{
  "source": ".",
  "destination": "./docs",
  "plugins": [
    { "name": "esdoc-standard-plugin" },
    {
      "name": "esdoc-ecmascript-proposal-plugin",
      "option": { "all": true }
    }
  ],
  "lifecycle": {
    "prebuild": "echo \"Starting documentation generation...\""
  },
  "excludes": ["(node_modules|docs|.idea|public)"]
}
